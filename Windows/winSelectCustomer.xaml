<Window x:Class="winSelectCustomer"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="winSelectCustomer" SizeToContent="WidthAndHeight" FontSize="18"  xmlns:local="clr-namespace:TKSISchedule" >
    <Grid>
        <local:viewCustomerSearch Margin="0,0,0,50"/>
        <Button Content="OK" Width="90" Margin="0,0,0,10" VerticalAlignment="Bottom" IsDefault="True" Command="{Binding CustDoubleClick}"/>
    </Grid>
</Window>
